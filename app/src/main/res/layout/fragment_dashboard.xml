<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.ui.dashboard.DashboardFragment" >


    <TextView
            android:text="@string/jine_text"
            android:layout_width="63dp"
            android:layout_height="42dp" android:id="@+id/textView5"
            android:textSize="18sp" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="@+id/money_text" app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintVertical_bias="0.0" app:layout_constraintEnd_toStartOf="@+id/money_text"
            android:textAlignment="center"/>
    <TextView
            android:layout_width="146dp"
            android:layout_height="43dp" android:id="@+id/local_time"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.032" app:layout_constraintHorizontal_bias="0.498"
            android:gravity="center" android:textSize="18sp"/>
    <TextView
            android:text="@string/leibie_text"
            android:layout_width="63dp"
            android:layout_height="42dp" android:id="@+id/textView4"
            android:textSize="18sp" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="@+id/money_text" app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintVertical_bias="0.116" app:layout_constraintEnd_toStartOf="@+id/money_text"
            android:textAlignment="center"/>
    <TextView
            android:text="@string/beizhu_text"
            android:layout_width="63dp"
            android:layout_height="42dp" android:id="@+id/textView6"
            android:textSize="18sp" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="@+id/money_text" app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintVertical_bias="0.226" app:layout_constraintEnd_toStartOf="@+id/money_text"
            android:textAlignment="center"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="date"
            android:ems="10"
            android:id="@+id/money_text"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.489" app:layout_constraintVertical_bias="0.234"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPostalAddress"
            android:ems="10"
            android:id="@+id/type_text"
            app:layout_constraintStart_toEndOf="@+id/textView4" android:layout_marginEnd="101dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="19dp"
            app:layout_constraintTop_toBottomOf="@+id/money_text" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="516dp" app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintVertical_bias="0.0"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPostalAddress"
            android:ems="10"
            android:id="@+id/remake_text"
            app:layout_constraintStart_toEndOf="@+id/textView6" android:layout_marginEnd="101dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="451dp" android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@+id/type_text"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintVertical_bias="0.0"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/pro_name_text"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="97dp"
            android:layout_marginEnd="101dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginBottom="22dp" app:layout_constraintBottom_toTopOf="@+id/money_text"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintVertical_bias="1.0"/>
    <TextView
            android:text="项目名称:"
            android:layout_width="63dp"
            android:layout_height="42dp" android:id="@+id/textView8"
            app:layout_constraintEnd_toStartOf="@+id/pro_name_text"
            app:layout_constraintTop_toTopOf="@+id/pro_name_text" android:textAlignment="center"
            android:textSize="14sp" android:layout_marginTop="12dp"/>
    <Button
            android:text="收入"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/income_button"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.3" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.554"/>
    <Button
            android:text="支出"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/exepenses_button"
            android:layout_marginStart="37dp" app:layout_constraintStart_toEndOf="@+id/income_button"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginEnd="101dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintVertical_bias="0.554"/>

</androidx.constraintlayout.widget.ConstraintLayout>